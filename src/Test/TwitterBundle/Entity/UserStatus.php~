<?php
namespace Test\TwitterBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="userstatus")
 */
class UserStatus{

    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     */
    protected $userId;

    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     */
    protected $statusId;
}